{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/inter.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/instagram",
      "destination": "https://www.instagram.com/gfung/"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/gfung_"
    },
    {
      "source": "/github",
      "destination": "https://github.com/gfungx"
    }
  ]
}